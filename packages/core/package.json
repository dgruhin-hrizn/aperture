{
  "name": "@aperture/core",
  "version": "0.2.5",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./db": {
      "import": "./dist/db/index.js",
      "types": "./dist/db/index.d.ts"
    },
    "./media": {
      "import": "./dist/media/index.js",
      "types": "./dist/media/index.d.ts"
    },
    "./recommender": {
      "import": "./dist/recommender/index.js",
      "types": "./dist/recommender/index.d.ts"
    },
    "./strm": {
      "import": "./dist/strm/index.js",
      "types": "./dist/strm/index.d.ts"
    },
    "./channels": {
      "import": "./dist/channels/index.js",
      "types": "./dist/channels/index.d.ts"
    },
    "./jobs": {
      "import": "./dist/jobs/index.js",
      "types": "./dist/jobs/index.d.ts"
    },
    "./watching": {
      "import": "./dist/watching/index.js",
      "types": "./dist/watching/index.d.ts"
    }
